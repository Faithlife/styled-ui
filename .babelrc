{
  "presets": [
    "react",
    [
      "env",
      {
        "modules": false,
        "targets": { "browsers": [">1%"] }
      }
    ],
    "stage-0"
  ],
  "plugins": [
    [
      "babel-plugin-styled-components",
      {
        "ssr": true
      }
    ],
	"react-docgen",
	"babel-plugin-transform-flow-strip-types"
  ],
  "env": {
    "test": {
      "presets": ["react", "env", "stage-0"]
    }
  }
}
