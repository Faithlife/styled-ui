{
	"extends": [
		"faithlife",
		"plugin:import/errors",
		"prettier"
	],
	"plugins": [
		"import"
	],
	"rules": {
		"jsx-a11y/label-has-for": 0,
		"react/jsx-filename-extension": 0
	},
	"settings": {
		"import/ignore": [
			"node_modules",
			".(less|css)$"
		],
		"import/resolver": "webpack"
	}
}
