{
	"name": "@faithlife/styled-ui",
	"version": "6.2.3",
	"main": "dist/main.js",
	"repository": "git@github.com:Faithlife/styled-ui.git",
	"license": "MIT",
	"description": "UI components for use in Faithlife applications.",
	"scripts": {
		"deploy-site": "npm run catalog-build && storybook-to-ghpages --ci --existing-output-dir=catalog/build",
		"eslint": "eslint ./components ./catalog --ext .js --ext .jsx",
		"test": "npm run build && npm run unit-test && npm run eslint && npm run stylelint",
		"precommit": "prettier --write \"./components/**/*.{js,jsx}\" && npm run stylelint",
		"build": "rimraf dist && webpack --mode=none",
		"prepublishOnly": "npm test",
		"postpublish": "yarn helmsman ship-to @faithlife/styled-ui@5.73.4 latest --no-ship-faithlife",
		"stylelint": "stylelint ./components/**/*.jsx",
		"unit-test": "cross-env NODE_ENV=test mocha -t 60000 -r @babel/register ./tests/**/*.js",
		"helmsman": "npx --userconfig ./.npmrc @faithlife/helmsman",
		"ship-to": "yarn helmsman ship-to @faithlife/styled-ui",
		"tag-for-beta": "yarn helmsman ship-to @faithlife/styled-ui beta --no-ship-faithlife",
		"tag-for-production": "yarn helmsman ship-to @faithlife/styled-ui production --no-ship-faithlife",
		"get-versions": "yarn helmsman get-versions @faithlife/styled-ui",
		"catalog-start": "catalog start",
		"catalog-build": "catalog build --public-url /styled-ui",
		"catalog-build-netlify": "npm run prepublishOnly && catalog build"
	},
	"files": [
		"dist/**",
		"text-input.js",
		"icons/*.js"
	],
	"browserslist": [
		"> 1%",
		"not op_mini all"
	],
	"peerDependencies": {
		"react": "^16.8.0",
		"react-dom": "^16.8.0",
		"styled-components": "^4 || ^5"
	},
	"peerDependenciesMeta": {
		"ag-grid-community": {
			"optional": true
		},
		"ag-grid-enterprise": {
			"optional": true
		},
		"ag-grid-react": {
			"optional": true
		}
	},
	"dependencies": {
		"@popperjs/core": "^2.4.0",
		"@styled-system/prop-types": "^5.1.1",
		"@styled-system/theme-get": "^5.1.1",
		"clipboard": "^2.0.1",
		"faithlife-react-popper": "^2.2.4",
		"focus-visible": "^5.0.1",
		"lodash.debounce": "^4.0.8",
		"lodash.throttle": "^4.1.1",
		"memoize-one": "^5.0.0",
		"prop-types": "^15.6.1",
		"react-select": "^3.1.1",
		"react-transition-group": "^4.4.1",
		"styled-system": "^5.1.1"
	},
	"devDependencies": {
		"@babel/core": "^7.5.4",
		"@babel/plugin-proposal-class-properties": "^7.5.0",
		"@babel/plugin-syntax-dynamic-import": "^7.2.0",
		"@babel/preset-env": "^7.5.4",
		"@babel/preset-react": "^7.0.0",
		"@babel/register": "^7.4.4",
		"@storybook/storybook-deployer": "^2.8.7",
		"@svgr/webpack": "^4.3.3",
		"assert": "^1.4.1",
		"babel-core": "^7.0.0-bridge.0",
		"babel-eslint": "^10.0.2",
		"babel-loader": "^8.0.6",
		"babel-plugin-add-module-exports": "^1.0.2",
		"babel-plugin-react-docgen": "^3.1.0",
		"babel-plugin-styled-components": "^1.10.6",
		"babel-runtime": "^6.26.0",
		"catalog": "^3.5.4",
		"chrono-node": "^1.3.5",
		"cross-env": "^5.2.0",
		"date-fns": "^2.0.0-alpha.25",
		"eslint": "^5.16.0",
		"eslint-config-faithlife": "^6.0.0",
		"esm": "^3.0.24",
		"micro-graphql-react": "^0.3.0-beta2",
		"mocha": "^5.1.1",
		"prettier": "^1.17.1",
		"react": "^16.8.0",
		"react-dom": "^16.8.0",
		"react-dom-factories": "^1.0.2",
		"resize-observer-polyfill": "^1.5.1",
		"rimraf": "^2.6.2",
		"styled-components": "^5.2.1",
		"stylelint": "^10.1.0",
		"stylelint-config-styled-components": "^0.1.1",
		"stylelint-processor-styled-components": "^1.8.0",
		"webpack": "^4.15.1",
		"webpack-cli": "^3.0.8",
		"webpack-node-externals": "^1.7.2"
	},
	"resolutions": {
		"babel-core": "7.0.0-bridge.0"
	}
}
